{
    "name": "cariuk/repository-pattern-laravel",
    "version": "1.0.8",
    "description": "CRUD wrapper for laravel apps",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hade Mopilie",
            "email": "hademopilie@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cariuk\\RepositoryPatternLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cariuk\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cariuk\\RepositoryPatternLaravel\\RepositoryPatternLaravelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
