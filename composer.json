{
    "version": "2.0.0",
    "description": "CRUD wrapper for laravel apps",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hade Mopilie",
            "email": "hademopilie@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RepositoryPatternLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RepositoryPatternLaravel\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RepositoryPatternLaravel\\RepositoryPatternLaravelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}